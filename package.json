{
  "name": "@jgornick/rnw-starter",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "0.60.5"
  },
  "description": "React Native Web Starter",
  "author": "Joe Gornick <joe@joegornick.com>",
  "license": "MIT",
  "scripts": {
    "start:emulator:android": "yarn workspace @jgornick/rnw-starter-native emulator:android:start",
    "start:native": "yarn workspace @jgornick/rnw-starter-native start",
    "start:storybook:native:android": "stmux -w always -- [ \"yarn start:emulator:android\" : \"yarn start:native\" ]",
    "start:storybook:web": "yarn workspace @jgornick/rnw-starter-web start"
  },
